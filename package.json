{
  "name": "@fastify/opensearch",
  "version": "2.0.0",
  "description": "Fastify plugin for opensearch",
  "main": "index.js",
  "type": "commonjs",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down",
    "test": "npm run test:unit && npm run test:typescript",
    "test:unit": "c8 --100 node --test",
    "test:typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-opensearch.git"
  },
  "keywords": [
    "fastify",
    "open",
    "opensearch",
    "open search",
    "opendistro",
    "open distro"
  ],
  "author": "RodrigoDornelles - @rodrigodornelles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-opensearch/issues"
  },
  "homepage": "https://github.com/fastify/fastify-opensearch#readme",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/fastify"
    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/fastify"
    }
  ],
  "dependencies": {
    "@opensearch-project/opensearch": "^3.3.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "c8": "^10.1.3",
    "eslint": "^9.17.0",
    "fastify": "^5.2.1",
    "neostandard": "^0.12.0",
    "tsd": "^0.33.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
